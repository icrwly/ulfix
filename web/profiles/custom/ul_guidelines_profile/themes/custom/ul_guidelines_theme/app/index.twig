{# 1-column.twig #}
{% extends '@ul_guidelines_theme/1_core/1-column.twig' %}

{% block page %}
  <section class="wrapper">
    <h1>{{ title }}</h1>
  </section>
  <section class="wrapper">
    <h5>Utility</h5>
    <ul>
      {% for item in utilityLinks %}
        <li><a href="{{ item.link }}">{{ item.name }}</a></li>
      {% endfor %}
    </ul>

    <h5>Layouts</h5>
    <ul>
      {% for item in layoutLinks %}
        <li><a href="{{ item.link }}">{{ item.name }}</a></li>
      {% endfor %}
    </ul>
  </section>
{% endblock %}
