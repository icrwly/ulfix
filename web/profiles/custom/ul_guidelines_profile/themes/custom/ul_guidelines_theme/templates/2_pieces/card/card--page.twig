<a href="{{- card.linkHref -}}" class="card card--page">
  <div class="image-container">
    {% block card_image %}
      <img src="{{- card.image -}}" alt="{{- card.imageAlt -}}">
    {% endblock %}
  </div>
  <div class="content-container">
    {% block card_title %}
      <h3>{{- card.title -}}</h3>
    {% endblock %}
    {% block card_meta %}
      <p>{{- card.meta -}}</p>
    {% endblock %}
  </div>
</a>
