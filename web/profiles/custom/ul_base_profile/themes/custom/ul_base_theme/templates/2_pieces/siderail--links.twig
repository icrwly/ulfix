{# @file: siderail--links.twig #}
<div class="siderail--links">
  {% block content %}
    <h3>{{ title }}</h3>
    {% if items %}
      <ul>
      {% for item in items %}
        <li><a href="{{ item.link }}" title="{{ item.name }}">{{ item.name }}</a></li>
      {% endfor %}
      </ul>
    {% endif %}
  {% endblock %}
</div>
