{# t05-events-detail.twig #}
{% extends '@ul_base_theme/1_core/layout.twig' %}

{% block content %}
  {% embed '@ul_base_theme/1_core/page.twig' %}
    {% block page_top %}
      {% include '@ul_base_theme/4_regions/hero--detail.twig' with hero_detail|merge({image: '', is_wide: true}) only %}
    {% endblock %}
    {% block page %}
      {% include '@ul_base_theme/3_components/nav--sub-bar.twig' with { nav: nav_sub_bar } %}
      {% include '@ul_base_theme/5_layouts/t05-events-detail.twig' %}
    {% endblock %}
    {% block page_bottom %}
      {% include '@ul_base_theme/4_regions/section--cards--events.twig' with related_cards only %}
    {% endblock %}
  {% endembed %}
{% endblock %}
