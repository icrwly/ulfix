{% extends '@ul_enterprise_theme/page/page.html.twig' %}
{% block content %}
    {% embed '@ul_base_theme/1_core/page.twig' %}
        {% block header %}
            {% embed '@ul_base_theme/4_regions/header.twig' %}
                {% block header_top %}
                    {{ page.header_top }}
                {% endblock %}
                {% block header_bottom %}
                    {{ page.header_bottom }}
                {% endblock %}
            {% endembed %}
        {% endblock %}
        {% block page_top %}
            {{ page.breadcrumb }}
            {{ page.highlighted }}
            {{ page.top }}
        {% endblock %}
        {% block page %}
            {% embed "@ul_base_theme/4_regions/section.twig" %}
                {% set class = 'wrapper max-width--tablet' %}
                {% set titleBar={
                    'title': page['#title']
                } %}
                {% block section_body %}
                    {{ page.content }}
                {% endblock %}
            {% endembed %}
        {% endblock %}
        {% block page_bottom %}
            {{ page.bottom }}
        {% endblock %}
        {% block footer %}
            {% embed '@ul_enterprise_theme/4_regions/footer.twig' %}
                {% block footer_top %}
                    {{ page.footer_top }}
                {% endblock %}
                {% block footer_bottom %}
                    {{ page.footer_bottom }}
                {% endblock %}
            {% endembed %}
        {% endblock %}
    {% endembed %}
{% endblock %}
