{% embed '@ul_enterprise_theme/4_regions/section--carousel.twig'  %}
    {% set carousel_items = content.field_carousel_slide %}
    {% if content.field_hash_target %}
        {% set field_hash_target = content.field_hash_target %}
    {% endif %}
    {% if content.field_shared_bg_color.0 is not empty %}
      {% set class = class ~ ' section--bgcolor-' ~ content.field_shared_bg_color.0|render|lower|replace({' ': '-'}) %}
    {% endif %}
{% endembed %}
