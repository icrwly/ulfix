{# t02-landing-page-with-siderail.twig #}
{% extends '@ul_enterprise_theme/1_core/layout.twig' %}

{% block content %}
  {% embed '@ul_base_theme/1_core/page.twig' %}
    {% block page_top %}
      {% include '@ul_base_theme/3_components/nav--local.twig' with local_nav only %}
      {% include '@ul_base_theme/4_regions/hero--default.twig' with t02Hero only %}
    {% endblock %}
    {% block page %}
      {% include '@ul_base_theme/5_layouts/t02-landing-page-with-siderail.twig' %}
    {% endblock %}
    {% block page_bottom %}
      {% include '@ul_base_theme/4_regions/section--action-banner.twig' with t02ActionBanner only %}
      {% include '@ul_base_theme/3_components/nav--local.twig' with local_nav|merge({is_mobile: true}) only %}
    {% endblock %}
  {% endembed %}
{% endblock %}
